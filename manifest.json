{
  "manifest_version": 2,
  "name": "SmoothZoom",
  "version": "1.1",
  "description": "Smooth zoom web page without touchpad",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "smooth-zoom.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "smooth-zoom@allanchain.github.io"
    }
  }
}
